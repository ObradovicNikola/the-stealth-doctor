[node name="WelcomeMessage" parent="ObjectiveMessages" index="0" instance=ExtResource( 11 )]
message = "Welcome, stealth Doctor, to your first day of training. Try to follow objective marker. Move with  A, W, S and D."

[node name="DoorMessage" parent="ObjectiveMessages" index="1" instance=ExtResource( 11 )]
message = "Excellent. Now we'll practice interacting with objects. Stand in front of the door and click on it with the mouse to open."

[node name="NightvisionMessage" parent="ObjectiveMessages" index="2" instance=ExtResource( 11 )]
message = "You'll be working in the dark a lot, so you are provided with a night vision kit. Toggle it with space key. You don't see cameras and guards while in night vision mode."

[node name="SuspicionMessage" parent="ObjectiveMessages" index="3" instance=ExtResource( 11 )]
message = "Beware of the guards and cameras. You can use Q to hide in a box temporarily (you only have 3 per mission). Monitor your suspicion meter in the bottom right."

[node name="LockedDoorMessage" parent="ObjectiveMessages" index="4" instance=ExtResource( 11 )]
message = "Some doors might be locked. You will have to find a corresponding computer with a password. Use nightvision to see number markers."

[node name="ComputerMessage" parent="ObjectiveMessages" index="5" instance=ExtResource( 11 )]
message = "Here's the computer. If you click on it, it will show you the password for the corresponding locked door. Use nightvision to see the label number."

[node name="LootMessage" parent="ObjectiveMessages" index="6" instance=ExtResource( 11 )]
